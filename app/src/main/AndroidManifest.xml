        <activity
            android:name="br.com.pauloceami.cursoappvendas.versaofinal.Json.ExecutaExports"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name="br.com.pauloceami.cursoappvendas.versaofinal.Controllers.RecyclerViewCliente"
            android:label="@string/title_activity_recycler_view_cliente" />
        <activity
            android:name="br.com.pauloceami.cursoappvendas.versaofinal.Controllers.Lista_clientes"
